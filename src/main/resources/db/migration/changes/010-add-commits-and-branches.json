    {
      "changeSet": {
        "id": "010-add-unique-constraint-on-commits",
        "author": "Division7 (IntelliJ IDEA JPA Buddy)",
        "changes": [
          {
            "addUniqueConstraint": {
              "columnNames": "org, repo, branch, sha",
              "constraintName": "uc_commit_sha_branch_id",
              "tableName": "commits"
            }
          }
        ]
      }
    },
    {
      "changeSet": {
        "id": "010-add-index-on-commits",
        "author": "Division7 (IntelliJ IDEA JPA Buddy)",
        "changes": [
          {
            "createIndex": {
              "columns": [
                {
                  "column": {
                    "name": "org"
                  }
                },
                {
                  "column": {
                    "name": "repo"
                  }
                },
                {
                  "column": {
                    "name": "branch"
                  }
                },
                {
                  "column": {
                    "name": "commit_time"
                  }
                }
              ],
              "indexName": "idx_commit_filtering",
