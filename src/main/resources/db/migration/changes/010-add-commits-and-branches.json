            }
          }
        ]
      }
    },
    {
      "changeSet": {
        "id": "add-fk-constraint-on-commits",
        "author": "Division7 (IntelliJ IDEA JPA Buddy)",
        "changes": [
          {
            "addForeignKeyConstraint": {
              "baseColumnNames": "org,repo,branch",
              "baseTableName": "commits",
              "constraintName": "FK_COMMITS_BRANCH",
              "onDelete": "CASCADE",
              "referencedColumnNames": "org,repo,branch_name",
              "referencedTableName": "branches"
            }
